<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
    
    <managed-bean>
        <managed-bean-name>input</managed-bean-name>
        <managed-bean-class>managedbeans.InputBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>currentUser</property-name>
            <value>#{LoginFormBean.current}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>TaskBean</managed-bean-name>
        <managed-bean-class>managedbeans.TaskBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
		<managed-bean-name>CreateFunctionBean</managed-bean-name>
		<managed-bean-class>managedbeans.CreateFunctionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
    <managed-bean>
		<managed-bean-name>ProjectsBean</managed-bean-name>
		<managed-bean-class>managedbeans.ProjectsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ProjectDetailsBean</managed-bean-name>
		<managed-bean-class>managedbeans.ProjectDetailsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
    <managed-bean>
        <managed-bean-name>WorkPieceBean</managed-bean-name>
        <managed-bean-class>managedbeans.WorkPieceBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>currentUser</property-name>
            <value>#{LoginFormBean.current}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
		<managed-bean-name>CreateProjectBean</managed-bean-name>
		<managed-bean-class>managedbeans.CreateProjectBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
   	<managed-bean>
		<managed-bean-name>CreateCustomerBean</managed-bean-name>
		<managed-bean-class>managedbeans.CreateCustomerBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	   	<managed-bean>
		<managed-bean-name>CustomersBean</managed-bean-name>
		<managed-bean-class>managedbeans.CustomersBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>CreateUserBean</managed-bean-name>
		<managed-bean-class>managedbeans.CreateUserBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>UsersBean</managed-bean-name>
		<managed-bean-class>managedbeans.UsersBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>CreateMessageBean</managed-bean-name>
		<managed-bean-class>managedbeans.CreateMessageBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>currentUser</property-name>
			<value>#{LoginFormBean.current}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ViewMessageBean</managed-bean-name>
		<managed-bean-class>managedbeans.ViewMessageBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>currentUser</property-name>
			<value>#{LoginFormBean.current}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ViewChatBean</managed-bean-name>
		<managed-bean-class>managedbeans.ViewChatBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>currentUser</property-name>
			<value>#{LoginFormBean.current}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ViewChatsBean</managed-bean-name>
		<managed-bean-class>managedbeans.ViewChatsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>currentUser</property-name>
			<value>#{LoginFormBean.current}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ViewChatProjectsBean</managed-bean-name>
		<managed-bean-class>managedbeans.ViewChatProjectsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>currentUser</property-name>
			<value>#{LoginFormBean.current}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ViewChatManagedProjectsBean</managed-bean-name>
		<managed-bean-class>managedbeans.ViewChatManagedProjectsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>currentUser</property-name>
			<value>#{LoginFormBean.current}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ChatsProjectBean</managed-bean-name>
		<managed-bean-class>managedbeans.ChatsProjectBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>currentUser</property-name>
			<value>#{LoginFormBean.current}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>LoginFormBean</managed-bean-name>
		<managed-bean-class>managedbeans.LoginFormBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>DashboardBean</managed-bean-name>
		<managed-bean-class>managedbeans.DashboardBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>currentUser</property-name>
			<value>#{LoginFormBean.current}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>MessagesBean</managed-bean-name>
		<managed-bean-class>managedbeans.MessagesBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>currentUser</property-name>
			<value>#{LoginFormBean.current}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>GeneralBean</managed-bean-name>
		<managed-bean-class>managedbeans.GeneralBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>currentUser</property-name>
			<value>#{LoginFormBean.current}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ConnectedUsersBean</managed-bean-name>
		<managed-bean-class>managedbeans.ConnectedUsersBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>currentUser</property-name>
			<value>#{LoginFormBean.current}</value>
		</managed-property>
		<managed-property>
			<property-name>currentUser</property-name>
			<value>#{LoginFormBean.current}</value>
		</managed-property>
	</managed-bean>
	<application>
		<message-bundle></message-bundle>
		<locale-config>
			<supported-locale>fr</supported-locale>
		</locale-config>
	</application>
	
	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Connected</from-outcome>
			<to-view-id>/Pages/dashboard.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Unconnected</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>Disonnected</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>NewProject</from-outcome>
			<to-view-id>/Pages/newproject.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
            <from-outcome>NewTask</from-outcome>
            <to-view-id>/Pages/newtask.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
			<from-outcome>Projects</from-outcome>
			<to-view-id>/Pages/projects.xhtml</to-view-id>
		</navigation-case>	
		<navigation-case>
            <from-outcome>Tasks</from-outcome>
            <to-view-id>/Pages/tasks.xhtml</to-view-id>
        </navigation-case>  
        <navigation-case>
			<from-outcome>Customers</from-outcome>
			<to-view-id>/Pages/customers.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>NewCustomer</from-outcome>
			<to-view-id>/Pages/newcustomer.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
            <from-outcome>NewWorkPiece</from-outcome>
            <to-view-id>/Pages/newworkpiece.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
			<from-outcome>Users</from-outcome>
			<to-view-id>/Pages/users.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>NewUser</from-outcome>
			<to-view-id>/Pages/newuser.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>NewFunction</from-outcome>
			<to-view-id>/Pages/newfunction.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Messages</from-outcome>
			<to-view-id>/Pages/messages.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>NouveauMessage</from-outcome>
			<to-view-id>/Pages/newmessage.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>HistoriquesChatProjet</from-outcome>
			<to-view-id>/Pages/histochatproject.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>DetailsMessage</from-outcome>
			<to-view-id>/Pages/viewmessage.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>DetailsProject</from-outcome>
			<to-view-id>/Pages/projectDetails.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
            <from-outcome>DetailsTask</from-outcome>
            <to-view-id>/Pages/task.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
			<from-outcome>Dashboard</from-outcome>
			<to-view-id>/Pages/dashboard.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>DetailsChatProject</from-outcome>
			<to-view-id>/Pages/viewchatsproject.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>DetailsChatManagedProject</from-outcome>
			<to-view-id>/Pages/viewchatsmanagedproject.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>DetailsConversation</from-outcome>
			<to-view-id>/Pages/viewchat.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>ListeConversations</from-outcome>
			<to-view-id>/Pages/viewchats.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<converter>
		<converter-id>userConverter</converter-id>
		<converter-class>converter.UserConverter</converter-class>
	</converter>
	<converter>
		<converter-id>projectConverter</converter-id>
		<converter-class>converter.ProjectConverter</converter-class>
	</converter>
</faces-config>